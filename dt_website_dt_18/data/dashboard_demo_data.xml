    <record id="dashboard_category_brand_health" model="dashboard.category">
        <field name="name">Brand Health</field>
    </record>
    <record id="dashboard_category_performance" model="dashboard.category">
        <field name="name">Performance Tracking</field>
    </record>
    <record id="dashboard_category_customer_experience" model="dashboard.category">
